#!/usr/bin/python3
import math

# Function to check if a number is prime
def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

# Function to factorize n into p and q
def factorize(n):
    for i in range(2, int(math.isqrt(n)) + 1):
        if n % i == 0:
            factor1 = i
            factor2 = n // i
            
            if is_prime(factor1) and is_prime(factor2):
                print(f"{n}={factor2}*{factor1}")
                return
    print(f"Could not factorize {n}")

# Main function to read from a file and factorize each number
def main(filename):
    with open(filename, 'r') as file:
        for line in file:
            n = int(line.strip())
            factorize(n)

if __name__ == "__main__":
    import sys
    if len(sys.argv) != 2:
        print("Usage: python3 rsa.py <input_file>")
        sys.exit(1)
    
    input_file = sys.argv[1]
    main(input_file)
